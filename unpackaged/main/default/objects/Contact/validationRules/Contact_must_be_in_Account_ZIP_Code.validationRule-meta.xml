<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_must_be_in_Account_ZIP_Code</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(ISBLANK(AccountId))  &amp;&amp;  (MailingPostalCode &lt;&gt; Account.ShippingPostalCode)</errorConditionFormula>
    <errorMessage>The contact is NOT in the zip code of its account.</errorMessage>
</ValidationRule>
